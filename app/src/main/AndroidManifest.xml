<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.emotionalappuse">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".mainframe.MainFrame"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".loginframe.LoginActivity"
            android:label="@string/title_activity_login" />

        <service android:name=".client.ClientService" />

        <activity
            android:name=".loginframe.RegisterActivity"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".mainframe.ClientMainActivity"
            android:label="@string/title_activity_client_main" />
        <activity android:name=".chatframe.ChatActivity" />
        <activity
            android:name=".persionalinformation.PersionalInformationActivity"
            android:label="@string/title_activity_persional_information"></activity>
        <activity
            android:name=".persionalinformation.SearchExpert"
            android:label="SearchExpert">


        </activity>
    </application>

</manifest>